    function LoadPanels(e) {
        LoadAdministratorPanel(e);
        LoadStaffPanel(e);
        LoadMemberPanel(e);
    }
    function LoadAdministratorPanel(e) {

        ajaxGet("loaderdiv", '@Url.Action("LoadAdministrators","Member")', [], function (data) {

            $("#AdminstatorsBody").html(data);
        },
            function (error, data, status) { });
    }
   
    function LoadStaffPanel(e) {
        @*$("#StaffsBody").load('@Url.Action("LoadStaff", "Member")', function () {

