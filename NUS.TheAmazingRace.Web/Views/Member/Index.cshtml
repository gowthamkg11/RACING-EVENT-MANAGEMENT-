         ajaxGet("loaderdiv", '@Url.Action("LoadStaff", "Member")', [], function (data) {

             $("#StaffsBody").html(data);
        },
            function (error, data, status) { });
    }
    function LoadMemberPanel(e) {
        @*$("#MembersBody").load('@Url.Action("LoadMembers", "Member")', function () {

        });*@
                 ajaxGet("loaderdiv", '@Url.Action("LoadMembers", "Member")', [], function (data) {

