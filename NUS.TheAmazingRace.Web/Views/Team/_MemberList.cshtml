@model IEnumerable<NUS.TheAmagingRace.DAL.Member>
    <div id="memberLoad">
@if (Model.Count() > 0)
{
    foreach(var item in Model)
    {
       <div class="">
           <p>
               @item.MemberName &nbsp;&nbsp;<a href="#" class="glyphicon glyphicon-remove" style="color:red" id="deleteMember" onclick="deleteMember(@item.MemberId, @item.Team.TeamID)"></a>
            </p>
        </div>
    }

}
else
{
    <div class="">
        <br />
        <p> No Member to display </p>
        <p> Click Add Member</p>
    </div>
}
        </div>
